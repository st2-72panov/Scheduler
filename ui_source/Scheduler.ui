    <string notr="true">/*/////////////////////////////////////////////////////////////////////////////////////////////////////////////
    Layouts   */

#bgApp {	 background-color: #f8f9fa }
#bottomBar { background-color: #dee5ef }

 /*//////////////*/
/*Top Header*/
#topHeader { background-color: #7EAFE7 }

 /*//////////*/
/*Content*/
#pageContainer { background-color: #f8f9fa }



/*/////////////////////////////////////////////////////////////////////////////////////////////////////////////
    Menus    */

 /*////////////////*/
/* Top Buttons */
#topButtons { background-color: #f8f9fa; border: 2px solid #72A5DC;  border-radius: 5px; }

#topButtons .QPushButton { border: none;  border-radius: 5px; }
#topButtons .QPushButton:hover { background-color: #dde4e6; border-style: solid; border-radius: 4px; }
#topButtons .QPushButton:pressed { background-color: #cdd7da; border-style: solid; border-radius: 4px; }

 /*////////////////*/
/* Left Buttons */

#leftMenuBox0 { background-color: #7EAFE7 }
#leftMenuBox { 
	background-color: #f8f9fa; 
	border-top: 2px solid #72A5DC; 
	border-bottom: 2px solid #72A5DC
}
#leftMenuBox .QPushButton {	
	background-position: left center;
    background-repeat: no-repeat;
	border: none;
	border-left: 16px solid transparent;
	background-color: transparent;
	text-align: left;
	padding-left: 44px;
}
#leftMenuBox .QPushButton:hover { background-color: #dde4e6 }
#leftMenuBox .QPushButton:pressed { background-color: #cdd7da }


#settingsBtn {	
	background-position: left center;
    background-repeat: no-repeat;
	border: none;
	border-left: 16px solid transparent;
	background-color: transparent;
	text-align: left;
	padding-left: 44px;
}
#settingsBtn:hover { background-color: #78a8cf }
#settingsBtn:pressed { background-color: #79add4 }

 /*//////////*/
/*Settings*/
#settingsLayout {
	background-color: #f8f9fa;
	border-top: 2px solid #8cadc7
}



/*/////////////////////////////////////////////////////////////////////////////////////////////////////////////
    Page Container    */

#pageContainer .QLabel { color: #1F1F1F; font-size: 10pt }
#pageContainer .QTableView, QTreeView { border: 2px solid #7EAFE7; border-radius: 5px }
QScrollArea, QLineEdit, QTextEdit { border: none }

/* QTabWidget */
QTabWidget:pane {
	border: 2px solid #cad0d9;
	border-radius: 5px;
	padding: 7px;
	padding-top: 11px;
	padding-bottom: 11px
}
QTabWidget::tab-bar { left: 6px }
QTabBar::tab { 
	border: 2px solid #cad0d9;
	border-bottom: none;
	padding: 4px
}

QTabBar::tab:first { border-top-left-radius: 4px }
QTabBar::tab:last { border-top-right-radius: 4px }
QTabBar::tab:!first:!last { margin-left: -1px; margin-right: -1px }
QTabBar::tab:!selected { margin-top: 4px }
QTabBar::tab:first { margin-right: -1px }
QTabBar::tab:last { margin-left: -1px }
QTabBar::tab:selected {
	border: 2px solid #b5bac1;
	border-bottom: none;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
}

/*QSpinBox*/
QDateEdit, QSpinBox {
	border: 2px solid #cad0d9;
	border-radius: 4px;
	padding: 2px;
	font-size: 10pt
}
QDateEdit::up-button, QDateEdit::down-button,
QSpinBox::up-button, QSpinBox::down-button {	
	background-color: #fff;
	border: 2px solid #b6bbc3;
	margin: -2px;
	width: 10px
}
QDateEdit::up-button, QSpinBox::up-button { margin-bottom: -1px; border-top-right-radius: 4px }
QDateEdit::down-button, QSpinBox::down-button { margin-top: -1px; border-bottom-right-radius: 4px }
QDateEdit::up-arrow, QSpinBox::up-arrow { image: url(:/icons/images/icons/icon_arrow-up.png); }
QDateEdit::down-arrow, QSpinBox::down-arrow { image: url(:/icons/images/icons/icon_arrow-down.png); }

/*QCheckBox*/
QCheckBox::indicator:unchecked { image: url(:/icons/images/icons/icon_unchecked.png) }
QCheckBox::indicator:checked { image: url(:/icons/images/icons/icon_checked.png) }

/*QScrollBar*/
QScrollBar:vertical {
	border: 2px solid #7EAFE7;
	border-radius: 5px;
	background: #f8f9fa;
	width: 12px;
	margin: 17px 0 17px 0;
 }
QScrollBar::handle:vertical {
	background: #cad0d9;
	border: 1px solid #dee5ef;
	min-height: 25px;
	border-radius: 4px
}
QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
	border: 2px solid #cad0d9;
	background: #fff;
	height: 7px;
	subcontrol-origin: margin;
}
QScrollBar::add-line:vertical { subcontrol-position: bottom }
QScrollBar::sub-line:vertical { subcontrol-position: top }

QScrollBar::up-arrow:vertical { image: url(:/icons/images/icons/icon_arrow-up.png) }
QScrollBar::down-arrow:vertical { image: url(:/icons/images/icons/icon_arrow-down.png) }

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
	background: none;
}


